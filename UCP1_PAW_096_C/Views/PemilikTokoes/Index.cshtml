@model IEnumerable<UCP1_PAW_096_C.Models.PemilikToko>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Nama Pemiik Toko")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaPemilikToko)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPemilikToko">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPemilikToko">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPemilikToko">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
